<?xml version="1.0"?>
<Cisco-IOS-XR-ipv6-smiap-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-ipv6-smiap-cfg" revision="2016-07-04">
  <files>
    <file revision="2016-07-04" submodule="false">Cisco-IOS-XR-ipv6-smiap-cfg.yang</file>
  </files>
  <prefix>ipv6-smiap-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="inet">ietf-inet-types</import>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <ipv6-virtual config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/ipv6_smiap/virtual_ip/</xr-path>
    <xr-xml-map>ipv6_virtual_cfg:IPv6Virtual</xr-xml-map>
    <xr-task>network &amp; ipv6</xr-task>
    <vrfs config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/ipv6_smiap/virtual_ip/vrf/</xr-path>
      <xr-xml-map>ipv6_virtual_cfg:VRFTable</xr-xml-map>
      <xr-task>network &amp; ipv6</xr-task>
      <vrf config="true" key="vrf-name" node="list">
        <xr-path>/cfg/gl/ipv6_smiap/virtual_ip/vrf/&lt;ipv6_virtual_cfg_VRF_vrf&gt;/</xr-path>
        <xr-xml-map>ipv6_virtual_cfg:VRF</xr-xml-map>
        <xr-task>network &amp; ipv6</xr-task>
        <address config="true" is-key="false" node="container" presence="true">
          <xr-data-handler>ignore</xr-data-handler>
          <xr-is-packed/>
          <xr-path>/cfg/gl/ipv6_smiap/virtual_ip/vrf/&lt;ipv6_virtual_cfg_VRF_vrf&gt;/addr`</xr-path>
          <xr-xml-map>ipv6_virtual_cfg:Address</xr-xml-map>
          <xr-task>network &amp; ipv6</xr-task>
          <xr-pack-format>%s%d</xr-pack-format>
          <address config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-is-packed/>
            <xr-pack-format>%s%d</xr-pack-format>
            <xr-pack-elem-size>1</xr-pack-elem-size>
            <xr-path>/cfg/gl/ipv6_smiap/virtual_ip/vrf/&lt;ipv6_virtual_cfg_VRF_vrf&gt;/addr`0</xr-path>
            <xr-xml-map>ipv6_virtual_cfg:Address</xr-xml-map>
            <xr-task>network &amp; ipv6</xr-task>
            <xr-sch-type>IPV6AddressString</xr-sch-type>
            <type>
              <type-name>inet:ipv6-address-no-zone</type-name>
            </type>
            <mandatory>true</mandatory>
          </address>
          <prefix-length config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-is-packed/>
            <xr-pack-format>%s%d</xr-pack-format>
            <xr-pack-elem-size>1</xr-pack-elem-size>
            <xr-path>/cfg/gl/ipv6_smiap/virtual_ip/vrf/&lt;ipv6_virtual_cfg_VRF_vrf&gt;/addr`1</xr-path>
            <xr-xml-map>ipv6_virtual_cfg:PrefixLength</xr-xml-map>
            <xr-task>network &amp; ipv6</xr-task>
            <xr-sch-type>IPV6PrefixLength</xr-sch-type>
            <type>
              <type-name>xr:Ipv6-prefix-length</type-name>
            </type>
            <mandatory>true</mandatory>
          </prefix-length>
        </address>
        <vrf-name config="true" is-key="true" is-yang-key="true" node="leaf">
          <xr-data-handler>ignore</xr-data-handler>
          <xr-primitive-type>string</xr-primitive-type>
          <xr-path>/cfg/gl/ipv6_smiap/virtual_ip/vrf/&lt;ipv6_virtual_cfg_VRF_vrf&gt;/</xr-path>
          <xr-xml-map>ipv6_virtual_cfg:VRFName</xr-xml-map>
          <xr-task>network &amp; ipv6</xr-task>
          <xr-sch-type>VRF_Name</xr-sch-type>
          <xr-sch-arg-name>&lt;ipv6_virtual_cfg_VRF_vrf&gt;</xr-sch-arg-name>
          <type>
            <type-name>xr:Cisco-ios-xr-string</type-name>
          </type>
        </vrf-name>
      </vrf>
    </vrfs>
    <use-as-source-address config="true" is-key="false" is-yang-key="false" node="leaf">
      <xr-primitive-type>uint32</xr-primitive-type>
      <xr-path>/cfg/gl/ipv6_smiap/virtual_ip/use_as_src_addr</xr-path>
      <xr-xml-map>ipv6_virtual_cfg:UseAsSourceAddress</xr-xml-map>
      <xr-task>network &amp; ipv6</xr-task>
      <xr-sch-type>Configured</xr-sch-type>
      <type>
        <type-name>empty</type-name>
      </type>
    </use-as-source-address>
  </ipv6-virtual>
</Cisco-IOS-XR-ipv6-smiap-cfg>
